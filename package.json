{
  "name": "spend-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run dev:db\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:db": "kill-port --port 8000,9099,5001,8080,9000,5000,8085,9199,4000,4400,4500 && npx firebase emulators:start --import ./firebase-emulators-db --export-on-exit --only auth,firestore,storage"
  },
  "dependencies": {
    "antd": "^5.12.2",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "compressorjs": "^1.2.1",
    "dayjs": "^1.11.13",
    "firebase": "^10.7.1",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "styled-icons": "^10.47.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-tools": "^14.2.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.2.0"
  }
}
